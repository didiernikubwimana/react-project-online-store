);\n \n\n\n  useEffect(() => {\n    const fetchData = async () => {\n        try {\n            const { data } = await axios.get(productAPI + '/' + productId);\n            setProduct(data);\n            console.log(\"success 1\");\n            // const productMatch = data.product.find((x) => x.product.id === productId);\n            // console.log(\"success 2\");\n            // setProduct(productMatch);\n            console.log(\"success 3\");\n            console.log(product);\n            console.log(\"Success data\");\n          } catch (err) {\n            console.log(err);\n          }\n        };\n        fetchData();\n      }, []);\n\n  const addToCartHandler = () => {\n    props.history.push(`/cart/${productId}?qty=${qty}`);\n  };\n  return (\n\n        <div>\n          <Link to=\"/\">Back to home page</Link>\n          <div className=\"row top\">\n            <div className=\"col-2\">\n              <img\n                className=\"large\"\n                src={product.photo}\n                alt={product.productName}\n              ></img>\n            </div>\n            <div className=\"col-1\">\n              <ul>\n                <li>\n                  <h1>{product.productName}</h1>\n                </li>\n                <li>\n\n                    <h2>{product.reviews}</h2>\n  \n                </li>\n                <li>Pirce : ${product.price}</li>\n                <li>\n                  Description:\n                  <p>{product.description}</p>\n                </li>\n              </ul>\n            </div>\n            <div className=\"col-1\">\n              <div className=\"card card-body\">\n                <ul>\n                  <li>\n                    Seller{' '}\n                    {/* <h2>\n                      <Link to={`/seller/${product.seller.id}`}>\n                        {product.seller.seller.name}\n                      </Link>\n                    </h2> */}\n                    {/* <Rating\n                      rating={product.seller.seller.rating}\n                      numReviews={product.seller.seller.numReviews}\n                    ></Rating> */}\n                  </li>\n                  <li>\n                    <div className=\"row\">\n                      <div>Price</div>\n                      <div className=\"price\">${product.price}</div>\n                    </div>\n                  </li>\n                  <li>\n                    <div className=\"row\">\n                      <div>Status</div>\n                      <div>\n                        {product.quantityInStock > 0 ? (\n                          <span className=\"success\">In Stock</span>\n                        ) : (\n                          <span className=\"danger\">Unavailable</span>\n                        )}\n                      </div>\n                    </div>\n                  </li>\n                  {product.quantityInStock > 0 && (\n                    <>\n                      <li>\n                        <div className=\"row\">\n                          <div>Qty</div>\n                          <div>\n                            <TextField\n                            label=\"Qty\"\n                              value={qty}\n                              onChange={(e) => setQty(e.target.value)}\n                              variant=\"outlined\"\n                              select\n                            >\n                              {[...Array(product.quantityInStock).keys()].map(\n                                (x) => (\n                                  <MenuItem key={x + 1} value={x + 1}>\n                                    {x + 1}\n                                  </MenuItem>\n                                )\n                              )}\n                            </TextField>\n                          </div>\n                        </div>\n                      </li>\n                      <li>\n                        <Button\n                          onClick={addToCartHandler}\n                          // className=\"primary block\"\n                          color=\"primary\"\n                          variant=\"contained\"\n                          size=\"large\"\n                        >\n                          Add to Cart\n                        </Button>\n                      </li>\n                    </>\n                  )}\n                </ul>\n              </div>\n            </div>\n          </div>\n        \n        <Review key={productId} productId = {productId}/>\n        </div>\n  );\n}","/Users/didier/Desktop/projets/react-project-online-store/src/constants/Images.js",["372","373"],"\n\nconst Images=(theme) =>{\n    const image1 = \"https://source.unsplash.com/WLUHO9A_xik/600x900\"\n    const image2 = \"https://source.unsplash.com/WLUHO9A_xik/600x900\"\n    const image3 = \"https://source.unsplash.com/WLUHO9A_xik/600x900\"\n    const image4 = \"https://source.unsplash.com/WLUHO9A_xik/600x900\"\n\n\n   const randomImages = [\n        {\n            img: image1,\n            title: 'Computers',\n            author: 'team4author',\n        },\n        {\n            img: image2,\n            title: 'Food',\n            author: 'team4author',\n        },\n        {\n            img: image3,\n            title: 'Cars',\n            author: 'team4author',\n        },\n        {\n            img: image4,\n            title: 'Sheos',\n            author: 'author',\n        },\n    ];\n\n}","/Users/didier/Desktop/projets/react-project-online-store/src/actions/cartActions.js",["374"],"import Axios from 'axios';\nimport {\n  CART_ADD_ITEM,\n  CART_REMOVE_ITEM,\n  CART_SAVE_SHIPPING_ADDRESS,\n  CART_SAVE_PAYMENT_METHOD,\n  CART_ADD_ITEM_FAIL,\n} from '../constants/constants';\n\n\nexport const addToCart = (productId, qty) => async (dispatch, getState) => {\n \n  const { dataUsed } = await Axios.get(`http://localhost:8080/api/products` + '/' + productId);\n  const {\n    cart: { cartItems },\n  } = getState();\n  if (cartItems.length < 0) {\n    dispatch({\n      type: CART_ADD_ITEM_FAIL,\n      payload: `Can't Add Item To Cart. `,\n    });\n  } else {\n    dispatch({\n      type: CART_ADD_ITEM,\n      payload: {\n        name: dataUsed.productName,\n        image: dataUsed.photo,\n        price: dataUsed.price,\n        countInStock: dataUsed.quantityInStock,\n        product: dataUsed.id,\n        seller: dataUsed.seller,\n        qty,\n      },\n    });\n    localStorage.setItem(\n      'cartItems',\n      JSON.stringify(getState().cart.cartItems)\n    );\n  }\n};\n\nexport const removeFromCart = (productId) => (dispatch, getState) => {\n  dispatch({ type: CART_REMOVE_ITEM, payload: productId });\n  localStorage.setItem('cartItems', JSON.stringify(getState().cart.cartItems));\n};\n\nexport const saveShippingAddress = (dataUsed) => (dispatch) => {\n  dispatch({ type: CART_SAVE_SHIPPING_ADDRESS, payload: dataUsed });\n  localStorage.setItem('shippingAddress', JSON.stringify(dataUsed));\n};\nexport const savePaymentMethod = (dataUsed) => (dispatch) => {\n  dispatch({ type: CART_SAVE_PAYMENT_METHOD, payload: dataUsed });\n};","/Users/didier/Desktop/projets/react-project-online-store/src/components/Product/Products.js",[],"/Users/didier/Desktop/projets/react-project-online-store/src/components/Review/Review.js",["375"],{"ruleId":"376","severity":1,"message":"377","line":1,"column":8,"nodeType":"378","messageId":"379","endLine":1,"endColumn":12},{"ruleId":"376","severity":1,"message":"380","line":2,"column":37,"nodeType":"378","messageId":"379","endLine":2,"endColumn":47},{"ruleId":"376","severity":1,"message":"381","line":3,"column":23,"nodeType":"378","messageId":"379","endLine":3,"endColumn":34},{"ruleId":"376","severity":1,"message":"382","line":5,"column":26,"nodeType":"378","messageId":"379","endLine":5,"endColumn":32},{"ruleId":"376","severity":1,"message":"383","line":5,"column":34,"nodeType":"378","messageId":"379","endLine":5,"endColumn":40},{